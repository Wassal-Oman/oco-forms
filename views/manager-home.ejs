<!DOCTYPE html>
<html lang="ar" dir="rtl">

<head>

    <!-- Head Partial -->
    <%- include('partials/head'); %>

    <title>استمارات الباحث الاجتماعي</title>
</head>

<body class="">
  <div class="wrapper ">
    <div class="sidebar" data-color="purple" data-background-color="white" data-image="/img/sidebar.jpg">

      <!-- Logo -->
      <div class="logo">
        <a href="/" class="simple-text logo-normal">
        استمارات الباحث الاجتماعي
        </a>
      </div>

      <!-- Sidebar Partial -->
      <%- include('partials/sidebar'); %>

    </div>
    <div class="main-panel">

      <!-- Navbar -->
      <nav class="navbar navbar-expand-lg navbar-transparent navbar-absolute fixed-top ">
        <div class="container-fluid">
          <div class="navbar-wrapper">
            <p class="navbar-brand">الرئيسية</p>
          </div>
          <button class="navbar-toggler" type="button" data-toggle="collapse" aria-controls="navigation-index" aria-expanded="false"
            aria-label="Toggle navigation">
            <span class="sr-only">Toggle navigation</span>
            <span class="navbar-toggler-icon icon-bar"></span>
            <span class="navbar-toggler-icon icon-bar"></span>
            <span class="navbar-toggler-icon icon-bar"></span>
          </button>
        </div>
      </nav>


      <div class="content">
        <div class="container-fluid">
          <div class="row">
            <div class="col-lg-3 col-md-6 col-sm-6">
              <div class="card card-stats">
                <div class="card-header card-header-warning card-header-icon">
                  <div class="card-icon">
                    <i class="material-icons">flash_on</i>
                  </div>
                  <p class="card-category">الكوارث الطبيعية</p>
                  <h3 class="card-title">120</h3>
                </div>
                <div class="card-footer">
                  <div class="stats">
                    <i class="material-icons">file_copy</i>عدد استمارات الكوارث الطبيعية
                  </div>
                </div>
              </div>
            </div>
            <div class="col-lg-3 col-md-6 col-sm-6">
              <div class="card card-stats">
                <div class="card-header card-header-success card-header-icon">
                  <div class="card-icon">
                    <i class="material-icons">place</i>
                  </div>
                  <p class="card-category">المزيونة</p>
                  <h3 class="card-title">300</h3>
                </div>
                <div class="card-footer">
                  <div class="stats">
                    <i class="material-icons">file_copy</i>عدد استمارات المزيونة
                  </div>
                </div>
              </div>
            </div>
            <div class="col-lg-3 col-md-6 col-sm-6">
              <div class="card card-stats">
                <div class="card-header card-header-danger card-header-icon">
                  <div class="card-icon">
                    <i class="material-icons">spa</i>
                  </div>
                  <p class="card-category">الاستماراة الاغاثية</p>
                  <h3 class="card-title">75</h3>
                </div>
                <div class="card-footer">
                  <div class="stats">
                    <i class="material-icons">file_copy</i>عدد الاستمارات العامة 
                  </div>
                </div>
              </div>
            </div>
            <div class="col-lg-3 col-md-6 col-sm-6">
              <div class="card card-stats">
                <div class="card-header card-header-info card-header-icon">
                  <div class="card-icon">
                    <i class="material-icons">public</i>
                  </div>
                  <p class="card-category">الاستمارة العامة</p>
                  <h3 class="card-title">100</h3>
                </div>
                <div class="card-footer">
                  <div class="stats">
                    <i class="material-icons">file_copy</i>عدد الاستمارات
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <footer class="footer">
          <div class="container-fluid">
            <div class="copyright float-right">
              &copy;
              <script>
                document.write(new Date().getFullYear())
              </script>
             كل الحقوق محفوظة لدى الهيئة العمانية للاعمال الخيرية
            </div>
          </div>
        </footer>
      </div>
    </div>

    <!-- Bottom Partial -->
    <%- include('partials/bottom'); %>
</body>

</html>
